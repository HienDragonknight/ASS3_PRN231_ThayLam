  "components": {
    "schemas": {
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "OrchidCreateDto": {
        "required": [
          "categoryId",
          "isNatural",
          "orchidDescription",
          "orchidName",
          "orchidUrl",
          "price"
        ],
        "type": "object",
        "properties": {
          "isNatural": {
            "type": "boolean"
          },
          "orchidDescription": {
            "minLength": 1,
            "type": "string"
          },
          "orchidName": {
            "minLength": 1,
            "type": "string"
          },
          "orchidUrl": {
            "minLength": 1,
            "type": "string"
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "categoryId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "OrchidUpdateDto": {
        "type": "object",
        "properties": {
          "isNatural": {
            "type": "boolean",
            "nullable": true
          },
          "orchidDescription": {
            "type": "string",
            "nullable": true
          },
          "orchidName": {
            "type": "string",
            "nullable": true
          },
          "orchidUrl": {
            "type": "string",
            "nullable": true
          },
          "price": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "categoryId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "OrderCreateDto": {
        "required": [
          "orderDetails"
        ],
        "type": "object",
        "properties": {
          "orderDetails": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrderDetailDto"
            }
          }
        },
        "additionalProperties": false
      },
      "OrderDetailDto": {
        "required": [
          "orchidId",
          "quantity"
        ],
        "type": "object",
        "properties": {
          "orchidId": {
            "type": "integer",
            "format": "int32"
          },
          "quantity": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "RegisterDto": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 6,
            "type": "string"
          },
          "accountName": {
            "type": "string",
            "nullable": true
          },
          "roleId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "JWT Authorization header using the Bearer scheme. Example: 'Bearer {token}'",
